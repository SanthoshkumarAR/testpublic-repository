{
  "name": "SampleApp",
  "version": "1.0.0",
  "description": "Sample todo app with react and java",
  "scripts": {
    "clean-dist": "npm run remove-dist && mkdir final",
    "start": "npm-run-all --parallel open:src",
    "open:src": "babel-node tools/server.js",
    "remove-dist": "node_modules/.bin/rimraf ./final",
    "prebuild": "npm-run-all build:html",
    "build:html": "babel-node tools/buildHtml.js",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "test": "mocha --reporter spec tools/testSetup.js \"template/**/*.test.js\"",
    "coverage": "istanbul cover template/reducers/platformsRegionsReducer.test.js",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Cognizant",
